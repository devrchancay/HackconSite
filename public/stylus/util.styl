#map-canvas
	height 400px

.parallax-container
	background-image url(../img/back.jpg)
	background-position center
	background-repeat no-repeat
	background-size cover
	overflow inherit !important
	padding 6em 0 0 0
	position relative
	height 530px
	&::before
		position absolute
		left 0
		top 0
		width 100%
		height 100%
		background-image -webkit-linear-gradient(top, rgba(97, 97, 97, 0.56) 0%, rgba(66, 66, 66, 0.56) 100%)
		background-image -o-linear-gradient(top, rgba(97, 97, 97, 0.56) 0%, rgba(66, 66, 66, 0.56) 100%)
		background-image linear-gradient(to bottom, rgba(97, 97, 97, 0.56) 0%, rgba(66, 66, 66, 0.56) 100%)
		background-repeat repeat-x
		content ""

.text-container
	position relative
	z-index 10

.typeout::after
	-webkit-animation 0.7s blink step-end infinite
	-moz-animation 0.7s blink step-end infinite
	-ms-animation 0.7s blink step-end infinite
	-o-animation 0.7s blink step-end infinite
	animation 0.7s blink step-end infinite
	content '|'
	padding-left 2px

.typeout.typeout-complete
	color #f50057
	font-weight bold

.typeout.typeout-complete:after
	content none


@keyframes blink
	from, to
		opacity 0
	50%
		opacity: 1

@-moz-keyframes blink
	from, to
		opacity 0
	50%
		opacity: 1

@-webkit-keyframes blink
	from, to
		opacity 0
	50%
		opacity: 1


.btnAction
	position absolute
	bottom -28px
	right 60px
